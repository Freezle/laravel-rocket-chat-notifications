{
    "name": "team-nifty-gmbh/laravel-rocket-chat-notifications",
    "description": "Rocket.Chat Notifications for laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "notification-channel",
        "rocketchat",
        "rocket.chat",
        "chat"
    ],
    "authors": [
        {
            "name": "Steffen Franz",
            "email": "sf@team-nifty.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^9.0",
        "illuminate/queue": "^9.0",
        "illuminate/support": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TeamNiftyGmbh\\RocketChatNotifications\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeamNiftyGmbh\\RocketChatNotifications\\RocketChatNotificationsServiceProvider"
            ]
        }
    }
}
